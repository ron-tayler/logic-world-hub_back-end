{
  "name": "@ron_tayler/logic-world-hub_back-end",
  "version": "0.1.0",
  "author": "Ron_Tayler",
  "license": "ISC",
  "private": true,
  "scripts": {
    "start:js": "node dist/main.js",
    "start:ts": "ts-node src/main.ts",
    "start:ts:dev": "ts-node src/Dev/main.ts",
    "build": "npm run build:clean && npm run build:ts",
    "build:ts": "tsc",
    "build:clean": "rimraf ./dist",
    "tests": "ava",
    "tests:watch": "ava --watch",
    "prisma:pull": "prisma db pull --schema prisma/schema.prisma",
    "prisma:push": "prisma db push --schema prisma/schema.prisma",
    "prisma:gen": "prisma generate --schema prisma/schema.prisma",
    "prisma:format": "prisma format --schema prisma/schema.prisma",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "prisma db seed",
    "reinstall": "rimraf ./node_modules & npm install"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "axios": "^0.26.1",
    "body-parser": "^1.20.1",
    "express": "^4.18.1",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "io-ts": "^2.2.20",
    "io-ts-types": "^0.5.19",
    "md5": "^2.3.0",
    "module-alias": "^2.2.2",
    "node-json-db": "^1.4.1",
    "redis": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "telegraf": "^4.8.5",
    "useragent": "^2.3.0",
    "validator": "^13.7.0",
    "yamlparser": "^0.0.2"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/events": "^3.0.0",
    "@types/express": "^4.17.14",
    "@types/md5": "^2.3.0",
    "@types/module-alias": "^2.0.1",
    "@types/useragent": "^2.3.0",
    "@types/validator": "^13.7.0",
    "ava": "^4.1.0",
    "prisma": "^3.12.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.5.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
